{
  "name": "Doraemay",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e2e",
    "editor.foreground": "#abb2bf",
    "sideBar.background": "#181825",
    "sideBar.foreground": "#abb2bf",
    "activityBar.background": "#11111b",
    "activityBar.foreground": "#abb2bf",
    "titleBar.activeBackground": "#181825",
    "titleBar.activeForeground": "#abb2bf",
    "statusBar.background": "#11111b",
    "statusBar.foreground": "#abb2bf",
    "tab.activeBackground": "#1e1e2e",
    "tab.inactiveBackground": "#181825",
    "editorGroupHeader.tabsBackground": "#181825",
    "terminal.background": "#1e1e2e",
    "terminal.foreground": "#abb2bf",
    "terminal.ansiBlack": "#373738",
    "terminal.ansiRed": "#e06c75",
    "terminal.ansiGreen": "#98c379",
    "terminal.ansiYellow": "#d19a66",
    "terminal.ansiBlue": "#61afef",
    "terminal.ansiMagenta": "#c678dd",
    "terminal.ansiCyan": "#56b6c2",
    "terminal.ansiWhite": "#abb2bf",
    "terminal.ansiBrightBlack": "#5c6370",
    "terminal.ansiBrightRed": "#e06c75",
    "terminal.ansiBrightGreen": "#98c379",
    "terminal.ansiBrightYellow": "#d19a66",
    "terminal.ansiBrightBlue": "#61afef",
    "terminal.ansiBrightMagenta": "#c678dd",
    "terminal.ansiBrightCyan": "#56b6c2",
    "terminal.ansiBrightWhite": "#ffffff"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#5A4B81" }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "punctuation.definition.keyword", "storage.modifier", "storage.type"],
      "settings": { "foreground": "#c678dd" }
    },
    {
      "name": "Function names",
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": { "foreground": "#98c379" }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted"],
      "settings": { "foreground": "#98c379" }
    },
    {
      "name": "User-defined class names",
      "scope": ["entity.name.type.class", "entity.name.class"],
      "settings": { "foreground": "#d19a66" }
    },
    {
      "name": "Language Built-ins",
      "scope": ["support"],
      "settings": { "fontStyle": "italic", "foreground": "#61afef" }
    },
    {
      "name": "Constants",
      "scope": ["constant", "variable.other.constant"],
      "settings": { "foreground": "#d19a66" }
    },
    {
      "name": "HTML tags",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#e06c75" }
    },
    {
      "name": "HTML/CSS attribute names",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#98c379", "fontStyle": "italic" }
    },
    {
      "name": "Function parameters",
      "scope": ["entity.name.variable.parameter", "variable.parameter"],
      "settings": { "fontStyle": "italic", "foreground": "#d19a66" }
    },
    {
      "name": "Variables and object properties",
      "scope": ["variable", "support.variable.property"],
      "settings": { "foreground": "#e06c75" }
    },
    {
      "name": "Brackets, braces, parens",
      "scope": ["meta.brace.round", "meta.brace.square", "meta.brace.curly", "punctuation.definition.arguments.begin", "punctuation.definition.arguments.end", "punctuation.definition.block.begin", "punctuation.definition.block.end"],
      "settings": { "foreground": "#5c6370" }
    },
    {
      "name": "Separators",
      "scope": ["punctuation.separator.key-value", "punctuation.separator.namespace"],
      "settings": { "foreground": "#e06c75" }
    },
    {
      "name": "this, super, self, etc.",
      "scope": ["keyword.other.this", "variable.language"],
      "settings": { "foreground": "#56b6c2", "fontStyle": "italic" }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": { "foreground": "#c678dd" }
    },
    {
      "name": "Constant escape sequences",
      "scope": ["constant.character.escape", "constant.regexp"],
      "settings": { "foreground": "#e06c75" }
    }
  ]
}
