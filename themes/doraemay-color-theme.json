{
  "name": "Doraemay",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e2e",
    "editor.foreground": "#cdd6f4",
    "sideBar.background": "#181825",
    "sideBar.foreground": "#cdd6f4",
    "activityBar.background": "#11111b",
    "activityBar.foreground": "#cdd6f4",
    "titleBar.activeBackground": "#181825",
    "titleBar.activeForeground": "#cdd6f4",
    "statusBar.background": "#11111b",
    "statusBar.foreground": "#cdd6f4",
    "tab.activeBackground": "#1e1e2e",
    "tab.inactiveBackground": "#181825",
    "editorGroupHeader.tabsBackground": "#181825",
    "terminal.background": "#11111b"
  },
"tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#5A4B81" }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "punctuation.definition.keyword"],
      "settings": { "foreground": "#FF79C6" }
    },
    {
      "name": "Function names",
      "scope": ["entity.name.function", "meta.function-call"],
      "settings": { "foreground": "#67e480" }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted"],
      "settings": { "foreground": "#e7de79" }
    },
    {
      "name": "User-defined class names",
      "scope": ["entity.name.type.class", "entity.name.class"],
      "settings": { "foreground": "#988bc7" }
    },
    {
      "name": "Language Built-ins",
      "scope": ["support"],
      "settings": { "fontStyle": "italic", "foreground": "#78D1E1" }
    },
    {
      "name": "Constants",
      "scope": ["constant", "variable.other.constant"],
      "settings": { "foreground": "#988bc7" }
    },
    {
      "name": "HTML tags",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#FF79C6" }
    },
    {
      "name": "HTML/CSS attribute names",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#67e480", "fontStyle": "italic" }
    },
    {
      "name": "Function parameters",
      "scope": ["entity.name.variable.parameter", "variable.parameter"],
      "settings": { "fontStyle": "italic", "foreground": "#E89E64" }
    },
    {
      "name": "Variables and object properties",
      "scope": ["variable", "support.variable.property"],
      "settings": { "foreground": "#E1E1E6" }
    },
    {
      "name": "Brackets, braces, parens",
      "scope": ["meta.brace.round", "punctuation.definition.arguments.begin", "punctuation.definition.arguments.end"],
      "settings": { "foreground": "#E1E1E6" }
    },
    {
      "name": "Separators",
      "scope": ["punctuation.separator.key-value", "punctuation.separator.namespace"],
      "settings": { "foreground": "#FF79C6" }
    },
    {
      "name": "this, super, self, etc.",
      "scope": ["keyword.other.this", "variable.language"],
      "settings": { "foreground": "#78D1E1", "fontStyle": "italic" }
    },
    {
      "name": "Storage",
      "scope": ["storage"],
      "settings": { "foreground": "#FF79C6" }
    },
    {
      "name": "Constant escape sequences",
      "scope": ["constant.character.escape", "constant.regexp"],
      "settings": { "foreground": "#FF79C6" }
    }
  ]
}
